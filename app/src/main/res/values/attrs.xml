<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OnOffView">
        <attr name="offColor" format="color" />   <!--开关关闭时颜色-->
        <attr name="onColor" format="color" />    <!--打开时颜色-->
        <attr name="backWith" format="dimension" />    <!--可以不要~~-->
        <attr name="iconSize" format="dimension" />    <!--小图标大小-->
        <attr name="paddingSize" format="dimension" />    <!--间距-->
        <attr name="defOff" format="boolean" />      <!--默认状态-->
        <attr name="enabled" format="boolean" />   <!--启用状态-->
    </declare-styleable>
    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetPadding" format="dimension" />
        <attr name="appWidgetInnerRadius" format="dimension" />
        <attr name="appWidgetRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="CollapseCardView">
        <attr name="collapse" format="boolean" />
        <attr name="collapseHeight" format="dimension" />
        <attr name="expandHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SegmentedControl">
        <attr name="radius" format="dimension" />
        <attr name="border" format="dimension"/>
        <attr name="textSize" format="dimension"/>
    </declare-styleable>
</resources>